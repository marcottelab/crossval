<%= f.error_messages %>

<p>
  <%= f.label :method %><br />
  <%= select_method f, :method %>
</p>

<div id='integrator_integrands'>
  <% f.fields_for :integrands do |integrand_form| %>
    <%= render :partial => 'integrators/integrand', :locals => { :f => integrand_form } %>
  <% end %>
</div>

<p>
  <%= add_source_link 'Add a Source', 'source', f %>
</p>

<% content_for :head do -%>
  <%= javascript_include_tag :defaults %>
<% end -%>

<h1>New experiment</h1>

<% form_for([:matrix, @experiment]) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', matrix_path(@matrix) %>
